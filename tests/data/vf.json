{
	"system": {
		"rsync": "/bin/overriden/rsync"
	},
	"vfs": [
		{
			"key": "/source",
			"path": "/target"
		},
		{
			"key": "/brol",
			"path": "/target/brolmachin"
		},
		{
			"key": "/truc",
			"path": "/target/trucmachin",
			"backup": "/target/backup",
			"readonly": true
		},
		{
			"key": "/ro",
			"path": "/target/ro",
			"readonly": true
		},
		{
			"key": "/restricted",
			"path": "/allowed",
			"users": [
				"admin"
			]
		},
		{
			"key": "/interpreted",
			"path": "/target/{key}/{user}/and/{timestamp}",
			"backup": "/target/{key}/{user}/backup/{timestamp}"
		},
		{
			"key": "/bitbucket/packages/bin/jehon-base-minimal",
			"path": "/target"
		},
		{
			"key": "/regex/{test}/truc",
			"path": "/xeger/{test}/bazar"
		},
		{
			"key": "/target/{user}/scripts/backup",
			"path": "/volume3/scripts/synology/public/{user}-backup.sh",
			"readOnly": true
		},
		{
			"key": "/target/data/{user}",
			"path": "/volume3/Backups/cron/{user}",
			"backup": "/volume3/Backups/cron/{user}-{timestamp}"
		},
		{
			"key": "/target/transferts/{user}",
			"path": "/volume3/transferts/auto/{user}/",
			"backup": "/volume3/transferts/auto/{user}/{timestamp}"
		},
		{
			"key": "/synology/rootssh",
			"path": "/root/.ssh"
		},
		{
			"key": "/synology/hosthooks",
			"path": "/volume3/scripts/hosthooks"
		},
		{
			"key": "/synology/scripts",
			"path": "/volume3/scripts/synology"
		},
		{
			"key": "/synology/kiosk",
			"path": "/volume3/web/"
		}
	]
}
