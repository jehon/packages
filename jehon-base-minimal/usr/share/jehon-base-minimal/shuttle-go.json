{
    "apps": [
        {
            "name": "vlc",
            "matchwindow_titles": [
                "VLC"
            ],
            "driver": "exec",
            "slow_jog": 0,
            "bindings": {
                "F1": "jh-vlc-control.sh show-timing",
                "F2": "true",
                "F3": "jh-vlc-control.sh next-audio",
                "F4": "jh-vlc-control.sh show-subtitle",
                "F5": "jh-vlc-control.sh play",
                "F6": "jh-vlc-control.sh pause",
                "F7": "true",
                "F8": "true",
                "F9": "true",
                "SlowJogL": "jh-vlc-control.sh prev-frame",
                "SlowJogR": "jh-vlc-control.sh next-frame",
                "S-7": "jh-vlc-control.sh start-prev-3",
                "S-6": "jh-vlc-control.sh start-prev-3",
                "S-5": "jh-vlc-control.sh start-prev-2",
                "S-4": "jh-vlc-control.sh start-prev-2",
                "S-3": "jh-vlc-control.sh start-prev-2",
                "S-2": "jh-vlc-control.sh start-prev-1",
                "S-1": "jh-vlc-control.sh start-prev-1",
                "S0": "jh-vlc-control.sh release",
                "S1": "jh-vlc-control.sh start-next-1",
                "S2": "jh-vlc-control.sh start-next-1",
                "S3": "jh-vlc-control.sh start-next-2",
                "S4": "jh-vlc-control.sh start-next-2",
                "S5": "jh-vlc-control.sh start-next-2",
                "S6": "jh-vlc-control.sh start-next-3",
                "S7": "jh-vlc-control.sh start-next-3"
            }
        }
    ]
}